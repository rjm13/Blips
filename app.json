{
  "expo": {
    "name": "Blip",
    "scheme": "test-scheme",
    "slug": "Blip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/blipicon.png",
    "splash": {
      "image": "./assets/images/blipsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/blipicon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/images/blipicon.png"
    }
  }
}
